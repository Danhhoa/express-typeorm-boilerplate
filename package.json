{
    "name": "express-typeorm-boilerplate",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "engines": {
        "node": ">=18.x"
    },
    "license": "danhhoa",
    "scripts": {
        "test": "jest --collectCoverage",
        "start": "yarn serve",
        "serve": "node dist/index.js",
        "dev": "nodemon",
        "watch-ts": "tsc -w",
        "build": "npm run clear && npm run build-ts && npm run build-babel",
        "clear": "rm -rf ./build && rm -rf ./dist",
        "build-ts": "npx tsc",
        "build-babel": "npx babel build --out-dir dist --source-maps",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
        "typeorm:win": "ts-node -r tsconfig-paths/register node_modules\\typeorm\\cli.js",
        "migration:generate": "npm run typeorm migration:generate src/typeorm/migrations/$npm_config_name -- -d src/typeorm/data-source.ts",
        "migration:create": "npm run typeorm migration:create src/typeorm/migrations/$npm_config_name",
        "migration:run": "npm run typeorm migration:run -- -d src/typeorm/data-source.ts",
        "migration:revert": "npm run typeorm migration:revert -- -d src/typeorm/data-source.ts",
        "migration:generate:win": "cross-var npm run typeorm:win migration:generate src/typeorm/migrations/$npm_config_name -- -d src/typeorm/data-source.ts",
        "migration:create:win": "cross-var npm run typeorm:win typeorm migration:create src/typeorm/migrations/$npm_config_name",
        "migration:run:win": "npm run typeorm:win migration:run -- -d src/typeorm/data-source.ts",
        "migration:revert:win": "npm run typeorm:win migration:revert -- -d src/typeorm/data-source.ts"
    },
    "devDependencies": {
        "@babel/cli": "^7.25.6",
        "@babel/core": "^7.25.2",
        "@babel/node": "^7.25.0",
        "@babel/plugin-syntax-import-source": "^7.24.7",
        "@babel/plugin-transform-runtime": "^7.25.4",
        "@babel/preset-env": "^7.25.4",
        "@babel/preset-typescript": "^7.24.7",
        "@types/bcrypt": "^5.0.0",
        "@types/body-parser": "^1.19.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/mailgun-js": "^0.22.18",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.11",
        "@types/supertest": "^6.0.2",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/uuid": "^9.0.8",
        "babel-loader": "^9.1.3",
        "babel-plugin-istanbul": "^7.0.0",
        "babel-plugin-root-import": "^6.6.0",
        "babel-polyfill": "^6.26.0",
        "babel-register": "^6.26.0",
        "babel-watch": "^7.8.1",
        "tsconfig-paths": "^4.2.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.583.0",
        "@types/lodash": "^4.17.7",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "cross-var": "^1.1.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-jwt": "^8.4.1",
        "express-winston": "^4.2.0",
        "http-status-codes": "^2.3.0",
        "joi": "^17.13.1",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "multer-s3": "^3.0.1",
        "nodemon": "^3.1.1",
        "pg": "^8.11.5",
        "prettier": "^3.2.5",
        "reflect-metadata": "^0.2.2",
        "swagger-ui-express": "^5.0.1",
        "ts-node": "^10.9.2",
        "tslint": "^6.1.3",
        "typeorm": "^0.3.20",
        "typeorm-naming-strategies": "^4.1.0",
        "typescript": "^4.1.3",
        "uuid": "^9.0.1",
        "winston": "^3.13.0"
    }
}
